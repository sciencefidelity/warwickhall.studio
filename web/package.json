{
  "name": "svelte-kit-template",
  "version": "0.1.0",
  "description": "TODO",
  "author": "Matt Cook <matt@sciencefidelity.co.uk>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview --host",
    "clean": "rm -rf .svelte-kit build package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:js": "eslint --ext .js,.ts,.svelte .",
    "lint:format": "prettier --check --plugin-search-dir=. '**/*.{ts,js,json,md,yml,svelte}'",
    "lint:styles": "stylelint '**/*.{svelte,scss}' --ignore-path .eslintignore",
    "fix": "concurrently 'yarn:fix:*'",
    "fix:js": "eslint --ext .js,.ts,.svelte --fix .",
    "fix:format": "prettier --write --plugin-search-dir=. '**/*.{ts,js,json,md,yml,svelte}'",
    "fix:styles": "stylelint --fix '**/*.{svelte,scss}' --ignore-path .eslintignore",
    "test": "echo \"This template does not include a test runner.\" && exit 1"
  },
  "dependencies": {
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "concurrently": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.45.2",
    "stylelint": "^14.2.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
