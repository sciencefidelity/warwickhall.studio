---
import "normalize.css"
import "styles/global.scss"
import Content from "components/Content.astro"
import Footer from "components/Footer.astro"
import Header from "components/Header.astro"
---
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Warwick Hall Studios | affordable artists studios in Cardiff</title>
  <meta name="Description" content="An artist led initiative providing high quality studio accommodation for artists and makers in Cardiff." />

  <link
    rel="preload"
    href="/assets/fonts/gtalpina-bold-sub.woff2"
    as="font"
    type="font/woff2"
    crossorigin
  />
    <link
    rel="preload"
    href="/assets/fonts/gtalpina-regular-lat.woff2"
    as="font"
    type="font/woff2"
    crossorigin
  />
    <link
    rel="preload"
    href="/assets/fonts/bananagrotesk-bold-sub.woff2"
    as="font"
    type="font/woff2"
    crossorigin
  />
    <link
    rel="preload"
    href="/assets/fonts/bananagrotesk-regular-lat.woff2"
    as="font"
    type="font/woff2"
    crossorigin
  />

  <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
  <link rel="mask-icon" href="/mask-icon.svg" color="#CF99FF" />
  <meta name="theme-color" content="#1B1C1F" />
</head>

<body>
  <Header />
  <Content />
  <Footer />
</body>
</html>
